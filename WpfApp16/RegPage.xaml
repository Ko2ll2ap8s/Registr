        <TextBox Name="FIO" HorizontalAlignment="Left" Margin="324,254,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="Password1" HorizontalAlignment="Left" Margin="324,147,0,0" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="Password2" HorizontalAlignment="Left" Margin="324,199,0,0" VerticalAlignment="Top" Width="120"/>
